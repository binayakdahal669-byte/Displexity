// DISP Test Program (.dis source file)
function main() {
    // Test basic functionality
    Array[] greeting = "Hello, DISP!";
    print(greeting);
    
    // Test arrays
    Array[] numbers = [1, 2, 3, 4, 5];
    print(numbers[0]);
    print(numbers[4]);
    
    // Test arithmetic
    result = 5 + 3 * 2;
    print(result);
    
    // Test conditionals
    if (result > 10) {
        print("Result is greater than 10");
    } else {
        print("Result is 10 or less");
    }
    
    // Test loops
    i = 0;
    while (i < 3) {
        print(i);
        i = i + 1;
    }
    
    return 0;
}